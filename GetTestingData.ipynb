   "source": [
    "import re\n",
    "ipdta= open('Dataset.txt').read() \n",
    "#source\n",
    "\n",
    "#get sentences\n",
    "ntext=re.split('\\n',ipdta) \n",
    "\n",
    "#destination file\n",
    "text_file = open(\"TestingData.txt\", \"w\")\n",
    "\n",
    "#put only 100k lines\n",
    "for i in range(0,100000):\n",
    "    text_file.write(ntext[i])\n",
    "\n",
    "text_file.close()\n",
    "\n",
    "print('TestingDataMade')"
   ]
  }
